	void M() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if (j==0 || j==(column-1) || ((i==j || i+j==raw-1) && i<=(column/2))) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
		System.out.println("--------------------");
	}
	void N() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if (j==0 || j==column-1 || i==j) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
		System.out.println("--------------------");
	}
