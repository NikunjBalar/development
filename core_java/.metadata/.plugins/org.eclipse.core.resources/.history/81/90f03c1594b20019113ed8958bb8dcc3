package com.java.assignment;
import java.util.Scanner;
public class Alphabets {
	public static void main(String[] args) {
		System.out.println("main methos started");
		
		Alphabets a=new Alphabets();
		a.A();
		a.B();
		a.C();
		a.D();
		a.E();
		a.F();
		a.G1();
		a.G2();
		a.H();
		a.I();
		a.J();
		a.K();
		a.L();
		a.M();
		a.N();
		a.O();
		a.P();
		a.Q();
		a.R();
		a.S();
		a.T();
		a.U();
		a.V();
		a.W();
		a.X();
		a.Y();
		a.Z();
		
		System.out.println("main method ended..");
	}
	void A() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if (((i==0 && j!=i) && j!=column-1) || (j==0 && j!=i) || (j==(column-1) && i!=0) || i==(raw/2)) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
		System.out.println("--------------------");
	}
	void B() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if (i==0 || j==0 || (j==(column-1) && i!=(raw/2)) || (i==(raw/2) && j!=(column-1)) || i==(raw-1)) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
		System.out.println("--------------------");
	}
	void C() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if (i==0 || j==0 || i==(raw-1)) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
		System.out.println("--------------------");
	}
	void D() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if (j==1 || (i==0 && j!=column-1) || (i==raw-1 && j!=column-1) || (j==(column-1) && i!=j && i+j!=column-1)) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
		System.out.println("--------------------");
	}
	void E() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if (i==0 || j==0 || i==(raw-1) || (i==(raw/2) && j<=column/2)) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
		System.out.println("--------------------");
	}
	void F() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if (i==0 || j==0 || (i==(raw/2) && j<=column/2)) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
