	void Y() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if ((i==j && i<=raw/2) || (j==column/2 && i>=raw/2 ) || (i+j==raw-1) && j>=column/2) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
		System.out.println("--------------------");
	}
	void Z() {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number of raw");
		int raw=s.nextInt();
		System.out.println("Enter number of column");
		int column=s.nextInt();
		for (int i = 0; i < raw; i++) {
			for (int j = 0; j < column; j++) {
				if (i==0 || i+j==raw-1 || i==(raw-1)) {
					System.out.print(" * ");
				}
				else {
					System.out.print("   ");
